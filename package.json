{
  "name": "ai-ml-portfolio",
  "version": "1.0.0",
  "description": "Modern AI/ML Portfolio with Interactive Features",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"nodemon server/server.js\" \"cd client && npm start\"",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install && cd ../ml-service && pip install -r requirements.txt"
  },
  "keywords": [
    "portfolio",
    "ai",
    "ml",
    "mern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "axios": "^1.6.0",
    "bootstrap-icons": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "mongoose": "^8.0.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
